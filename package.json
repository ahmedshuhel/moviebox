{
  "name": "moviebox",
  "version": "0.0.1",
  "description": "Simple enough yet another media server framework.",
  "main": "index.js",
  "author": "Munim Dibosh",
  "license": "MIT",
  "scripts": {
    "build-server": "rimraf dist/ && babel server/ --out-dir dist/server/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./yarn.lock,./.babelrc --copy-files",
    "start-server": "yarn build-server && node dist/server/index.js",
    "watch-server": "nodemon --watch server/ --exec 'yarn start-server'",
    "build-client": "webpack"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "jquery": "^3.3.1",
    "morgan": "^1.9.0",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "nodemon": "^1.15.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0"
  }
}
