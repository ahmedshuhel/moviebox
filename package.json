{
  "name": "moviebox",
  "version": "0.0.1",
  "description": "Simple enough yet another media server framework.",
  "main": "index.js",
  "author": "Munim Dibosh",
  "license": "MIT",
  "scripts": {
    "build::server": "rimraf dist/server/ && babel server/ --out-dir dist/server/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./yarn.lock,./.babelrc --copy-files",
    "start::server": "yarn build::server && node dist/server/index.js",
    "watch::server": "nodemon --watch server/ --exec 'yarn start::server'",
    "build::client": "webpack --progress",
    "watch::client": "webpack --progress --watch",
    "start:dev": "concurrently \"yarn watch::client\" \"yarn watch::server\"",
    "start:prod": "yarn build::server && yarn build::client && node dist/server/index.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1.0.4",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "jquery": "^3.3.1",
    "morgan": "^1.9.0",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.9",
    "file-loader": "^1.1.9",
    "node-sass": "^4.7.2",
    "nodemon": "^1.15.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0"
  }
}
